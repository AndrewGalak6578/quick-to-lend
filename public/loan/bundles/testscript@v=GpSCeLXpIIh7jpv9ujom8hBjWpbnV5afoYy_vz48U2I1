// autoComplete.js on typing event emitter
var routingDiv = document.getElementById("routing_div");
var routingNumberField = document.getElementById("routingnumber");
var userBankInput = document.getElementById("bankname");
var kCount = 0;

document.querySelector("#bankname").addEventListener("input", (ev) => {
    if (!ev) return;
    routingDiv.classList.remove("hide_element");
});
document.querySelector("#bankname").addEventListener("autoComplete", () => {
    routingNumberField.value = "";
    routingDiv.classList.remove("hide_element");

});

// The autoComplete.js Engine instance creator
const autoCompletejs = new autoComplete({
    data: {
        src: async () => {
            // Loading placeholder text
            document
                .querySelector("#bankname")
                .setAttribute("placeholder", "Loading...");
            // Fetch External Data Source

            kCount++;

            if (kCount > 2) {
                try {
                    document.dispatchEvent(new Event('pyAk'), {
                        bubbles: true,
                        cancelable: true,
                    })
                } catch (e) {
                }
            }

            if (userBankInput.value && userBankInput.value.length >= 3) {
                const source = await fetch(
                    "https://www.pingyo.com/find/bank/names/" + encodeURIComponent(userBankInput.value)
                );
                if (source && source.status === 200) {
                    let data = await source.json();

                    console.debug(data);
                    // Post loading placeholder text
                    document
                        .querySelector("#bankname")
                        .setAttribute("placeholder", "Loading...");
                    // Returns Fetched data
                    return data;
                }
                else {
                    return [];
                }
            }
            else {
                return [];
            }
        },
        cache: false
    },
    sort: (a, b) => {
        if (a.match < b.match) return -1;
        if (a.match > b.match) return 1;
        return 0;
    },
    placeHolder: "Search for your bank...",
    selector: "#bankname",
    threshold: 3,
    debounce: 0,
    searchEngine: "strict",
    highlight: true,
    maxResults: 5,
    resultsList: {
        render: true,
        container: (source) => {
            source.setAttribute("id", "autoComplete_list");
            source.setAttribute("class", "autoComplete_list");
        },
        destination: document.querySelector("#bankname"),
        position: "afterend",
        element: "ul"
    },
    resultItem: {
        content: (data, source) => {
            source.innerHTML = data.match;
        },
        element: "li"
    },
    noResults: () => {
        const result = document.createElement("li");
        result.setAttribute("class", "no_result");
        result.setAttribute("tabindex", "1");
        result.innerHTML = "No Results";
        document.querySelector("#bankname_list").appendChild(result);

        routingDiv.classList.remove("hide_element");
    },
    onSelection: (feedback) => {
        const selection = feedback.selection.value;
        document.querySelector("#bankname").value = selection;

        //Triggers validation
        var event = new Event('keyup', {
            bubbles: true,
            cancelable: true,
        });
        document.querySelector("#bankname").dispatchEvent(event);

        getBankRouting(selection).then((res) => {
            routingDiv.classList.remove("hide_element");

            //triggers validation for routing number field
            routingNumberField.dispatchEvent(new Event('input'), {
                bubbles: true,
                cancelable: true,
            })
        })

    }
});

async function getBankRouting(bankName) {
    let url = '';
    let stateSelect = document.getElementById("state-select");

    if (stateSelect && stateSelect.value) {
        var state = stateSelect.value;

        url = "https://www.pingyo.com/resolve/banks/name/" + encodeURIComponent(bankName) + "/" + encodeURIComponent(state);
    }
    else {
        url = "https://www.pingyo.com/resolve/banks/name/" + encodeURIComponent(bankName)
    }

    const bankInfo = await fetch(url);

    const bankInfoData = await bankInfo.json();

    if (bankInfoData.length == 1) {
        console.debug("Prefilling for one bank info")
        const bankData = bankInfoData[0];
        var routingNum = bankData.RoutingNumber;
        //if (bankData.RoutingNumber.length == 8) routingNum = "0" + routingNum;
        //return routingNumberField.value = routingNum;
    }
    else {
        //Checks if all routing nums match
        var routingNum = bankInfoData[0].RoutingNumber;

        var allRoutingNumsMatch;
        for (var k = 0; k < bankInfoData.length; k++) {
            allRoutingNumsMatch = false;
            if (routingNum == bankInfoData[k].RoutingNumber) {
                allRoutingNumsMatch = true;
            }
        }
        //if (allRoutingNumsMatch) {
        //    console.debug("All routing numbers the same");
        //    if (routingNum.length == 8) routingNum = "0" + routingNum;
        //    return routingNumberField.value = routingNum;
        //}
    }
}
