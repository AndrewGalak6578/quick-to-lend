function getAppExtensions(urlVars) {
    if (!urlVars) return;

    var urlKeys = ['la', 'term', 'fn', 'ln', 'em', 'campaign', 'id', 'subid'];
    var appExtensionArray = [];

    for (var key in urlVars) {
        //Use .indexOf method for compatibility with Internet Explorer
        if (urlKeys.indexOf(key) == -1) {
            appExtensionArray.push({
                "Name": key,
                "Value": urlVars[key]
            });
        }
    }

    return appExtensionArray;
}

function setQueryParams() {
    var queryParams = pingyo.getUrlVars();
    document.getElementById(queryParams.term);

    if (queryParams.la)
        document.getElementById("la").value = queryParams.la;
    else if (pingyo.getValue("la"))
        document.getElementById("la").value = pingyo.getValue("la");

    if (queryParams.fn)
        document.getElementById("firstname").value = queryParams.fn;
    else if (pingyo.getValue("fn"))
        document.getElementById("firstname").value = pingyo.getValue("fn");

    if (queryParams.ln)
        document.getElementById("lastname").value = queryParams.ln;
    else if (pingyo.getValue("ln"))
        document.getElementById("lastname").value = pingyo.getValue("ln");

    if (queryParams.em)
        document.getElementById("email").value = queryParams.em;
    else if (pingyo.getValue("em"))
        document.getElementById("email").value = pingyo.getValue("em");

    if (queryParams.term) {
        var term = "loan-months-" + queryParams.term;
        if (document.getElementById(term)) {
            document.getElementById(term).checked = true;
        }
    }
    //if (queryParams["loan-months-radio"]) {
    //    if (document.getElementById("loan-months-" + queryParams["loan-months-radio"])) {
    //        document.getElementById("loan-months-" + queryParams["loan-months-radio"]).checked = true;

    //    }
    //}

    //Triggers change on loan amount field for slider ui
    // Create a new 'change' event
    var event = new Event('change');

    // Dispatch it.
    document.getElementById("la").dispatchEvent(event);
    //End Triggers change on loan amount field for slider ui
}

