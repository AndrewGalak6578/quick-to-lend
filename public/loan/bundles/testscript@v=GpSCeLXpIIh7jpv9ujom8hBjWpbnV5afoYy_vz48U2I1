                        d = this._connectWith();
                    if (d && this.ready)
                        for (i = d.length - 1; i >= 0; i--)
                            for (s = (n = t(d[i], this.document[0])).length - 1; s >= 0; s--)
                                (o = t.data(n[s], this.widgetFullName)) &&
                                    o !== this &&
                                    !o.options.disabled &&
