// Hook up the form so we can prevent it from being posted
var form = document.querySelector("form#pingYoForm");
// Hook up the inputs to validate on the fly
var inputs = document.querySelectorAll("input, textarea, select");

//Checking form against constraints
for (var i = 0; i < inputs.length; ++i) {
    inputs.item(i).addEventListener("input", function (ev) {
        let v = ev.target.value;
        var errors = validate(form, constraints) || {};
        showErrorsForInput(this, errors[this.name]);
        //console.log(errors);
    });

    let currentObj = inputs.item(i);

    if (currentObj.type == 'text' || currentObj.type == 'tel' || currentObj.type == 'number' || currentObj.type == 'email') {
        currentObj.onpaste = function (e) { e.stopPropagation(); return false; };
    }
}

//Polyfill for IE to use 'startsWith'
if (!String.prototype.startsWith) {
    String.prototype.startsWith = function (searchString, position) {
        position = position || 0;
        return this.substr(position, searchString.length) === searchString;
    };
}
//Polyfill for IE to use 'startsWith'

//Custom validations
validate.validators.mobilePhoneValidator = function (value, options, key, attributes) {
    var mobilePhonePattern = /^\(?([2-9]{1}\d{2})\)?[- ]?(\d{3})[- ]?(\d{4})$/;
    var userInputMobileNumber = options.trim().replace(/[^0-9]/g, "");
    var isValid = mobilePhonePattern.test(userInputMobileNumber);

    return isValid ? null : "does not appear to be valid";
};

validate.validators.workPhoneValidator = function (value, options, key, attributes) {
    //var workPhonePattern = /^(\d){10,11}$/;
    var workPhonePattern = /^\(?([2-9]{1}\d{2})\)?[- ]?(\d{3})[- ]?(\d{4})$/; //For US
    var userInputWork = options.trim().replace(/[^0-9]/g, "");
    var isValid = workPhonePattern.test(userInputWork);

    return isValid ? null : "does not appear to be valid";
};

validate.validators.zipCodeValidator = function (value, options, key, attributes) {
    var postCodePattern = /^([Gg][Ii][Rr] 0[Aa]{2})|((([A-Za-z][0-9]{1,2})|(([A-Za-z][A-Ha-hJ-Yj-y][0-9]{1,2})|(([A-Za-z][0-9][A-Za-z])|([A-Za-z][A-Ha-hJ-Yj-y][0-9]?[A-Za-z])))) [0-9][A-Za-z]{2})$/i;
    var zipCodePattern = /(^\d{5}$)|(^\d{5}-\d{4}$)/;
    var userInputPostCode = options.trim();
    var isValid = zipCodePattern.test(userInputPostCode);

    return isValid ? null : "does not appear to be valid";
};

validate.validators.streetInputValidator = function (value, options, key, attributes) {
    var usAddressPattern = /\b\d{1,}[a-zA-Z]{3,}\b/;
    var usAddressPattern2 = /\b[a-zA-Z]{3,}\d{1,}\b/;
    var userInput = options.trim();
    var isValid = usAddressPattern.test(userInput) == false &&
        usAddressPattern2.test(userInput) == false &&
        userInput.length >= 5;

    return isValid ? null : "does not appear to be valid";
};

validate.validators.licenseNumberValidator = function (value, options, key, attributes) {
    //var hasDLRegex = /^[A-Z0-9]{5,15}$/g; //5,15 characters alpha numeric
    var hasDLRegex = /^[a-zA-Z0-9]{5,15}$/i; //5,15 characters alpha numeric
    var hasNumberPattern = /\d/g;
    var isValid = false;

    var normalize = options.replace(/\W/g, '').trim();
    var hasTwoNumbers = normalize.match(hasNumberPattern); //Returns an array of all digits found in a string (or user input)
    if (!hasTwoNumbers) {
        hasTwoNumbers = "";
    }

    isValid = hasDLRegex.test(normalize) && hasTwoNumbers.length >= 1;

    return isValid ? null : "does not appear to be valid";
};

validate.validators.netAmountValidator = function (value, options, key, attributes) {
    var isValid;
    var splitDecimal = options.split(".");

    if (splitDecimal.length == 2) {
        var normalize = splitDecimal[0].replace(/,/g, "");
        var decimal = "." + splitDecimal[1];
        var amountNumber = Number(normalize);

        normalize = normalize + decimal;
        isValid = (amountNumber >= 800 && amountNumber < 10000) && decimal.length <= 3;
    }
    else {
        var normalize = options.replace(/,/g, "");
        var amountNumber = Number(normalize);

        isValid = (amountNumber >= 800 && amountNumber < 10000);
    }

    return isValid ? null : "must be between $800 and $9999";
};

validate.validators.ssnValidator = function (value, options, key, attributes) {
    let ssnPattern = /^(\d{3}-\d{2}-\d{4})|(\d{3}\d{2}\d{4})$/;
    let userInput;
    let isValid = false;

    if (options != null && options != undefined) {
        userInput = options.trim().replace(/\W/g, '');

        isValid = (ssnPattern.test(userInput) && userInput.length === 9);

        return isValid ? null : "does not appear to be valid";
    }

};

//Alternative DOB validator
validate.validators.dobValidator = function (value, options, key, attributes) {
    var dobpattern = /^([1-9]|0?[1-9]|[1-3]{1}[0-9]{1})\/([1-9]|0?[1-9]|[1]{1}[0-2])\/(19|20)[0-9]{2}$/;
    var userinputdob = options.trim().replace(/^\/|\/$/g, '');

    //use dd/mm/yyyy format for uk
    //var age = parseInt(moment(options, 'D/M/YYYY', true).fromNow()); //For UK
    var age = parseInt(moment(options, 'M/D/YYYY', true).fromNow()); //For US

    var isValid = age >= 18 && age <= 110;
    return isValid ? null : "must be over 18 years (Format: MM/DD/YYYY)";
};
////Alternative DOB validator

validate.validators.routingNumberValidator = function (value, options, key, attributes) {
    var userInput = options.trim().replace(/\D/g, '');
    var isValid = false;
    let i, n, t, s;

    // First, remove any non-numeric characters.
    //s = userInput.replace(/\D/g, ''); ==>Alternative option to for loop
    t = "";
    for (i = 0; i < userInput.length; i++) {
        s = parseInt(userInput.charAt(i), 10);
        if (s >= 0 && s <= 9)
            t = t + s;
    }

    // Now run through each digit and calculate the total.
    n = 0;
    for (i = 0; i < t.length; i += 3) {
        n += parseInt(t.charAt(i), 10) * 3
            + parseInt(t.charAt(i + 1), 10) * 7
            + parseInt(t.charAt(i + 2), 10);
    }

    //If the resulting sum is an even multiple of ten (but not zero),
    //the aba routing number is good.

    if (n != 0 && n % 10 == 0)
        //return null;
        isValid = true;
    else {
        isValid = false;
    }

    // Check the length, it should be nine digits (or eight for certain banks, i.e. Bank of America).

    if (t.length !== 9) {
        isValid = false;
    }

    return isValid ? null : "does not appear to be valid";
};

validate.validators.firstNameValidator = function (value, options, key, attributes) {
    var userInput = options.trim();
    var lastName = document.getElementById("lastname").value.trim();

    var isValid = userInput.toUpperCase() !== lastName.toUpperCase();

    return isValid ? null : "and last name cannot match";
};

validate.validators.lastNameValidator = function (value, options, key, attributes) {
    var userInput = options.trim();
    var firstName = document.getElementById("firstname").value.trim();

    var isValid = userInput.toUpperCase() !== firstName.toUpperCase();

    return isValid ? null : "and first name cannot match";
};

//Trigger custom validations on user input
var mobilePhoneInput = document.getElementById("mobilephone");
//var homePhoneInput = document.getElementById("homephone");
var workphoneInput = document.getElementById("workphone");
var zipCodeInput = document.getElementById("zipcode");
var zipCodeFillInput = document.getElementById("zipcode-fill");
var dobInput = document.getElementById("dob");
var ssnInput = document.getElementById("ssn");
var dlnumberInput = document.getElementById("dlnumber");
var routingNumberInput = document.getElementById("routingnumber");
var firstNameInput = document.getElementById("firstname");
var lastNameInput = document.getElementById("lastname");
var streetInput = document.getElementById("housestreet");
//var houseNameNumberInput = document.getElementById("housenamenumber");

//var cardNumberInput = document.getElementById("offer_cardnumber");

firstNameInput.addEventListener("input", function (ev) {
    var nameerror = validate(form, { First_name: { firstNameValidator: ev.target.value } });
    return nameerror ? showErrorsForInput(this, nameerror[this.name]) : null;

});

lastNameInput.addEventListener("input", function (ev) {
    var nameerror = validate(form, { Last_name: { lastNameValidator: ev.target.value } });
    return nameerror ? showErrorsForInput(this, nameerror[this.name]) : null;

});

mobilePhoneInput.addEventListener("input", function (ev) {
    var value = ev.target.value.trim();
    var v = value.replace(/\D/g, '');

    //this check is required to enable backspacing
    if (ev.inputType != "deleteContentBackward") {
        if (v.match(/^\d{3}$/) != null) {
            ev.target.value = '(' + v + ') ';
        } else if (v.match(/^(\d{3})(\d{3})$/) != null) {
            ev.target.value += '-';
        }

        //Adds formatting onfinal input
        let m = v.match(/^(\d{3})(\d{3})(\d{4})$/);

        if (m) ev.target.value = '(' + m[1] + ") " + m[2] + "-" + m[3];
    }


    var phoneerror = validate(form, { Mobile_phone: { mobilePhoneValidator: ev.target.value } });
    return phoneerror ? showErrorsForInput(this, phoneerror[this.name]) : null;

});

workphoneInput.addEventListener("input", function (ev) {
    var value = ev.target.value.trim();
    var v = value.replace(/\D/g, '');

    //this check is required to enable backspacing
    if (ev.inputType != "deleteContentBackward") {
        if (v.match(/^\d{3}$/) != null) {
            ev.target.value = '(' + v + ') ';
        } else if (v.match(/^(\d{3})(\d{3})$/) != null) {
            ev.target.value += '-';
        }

        //Adds formatting onfinal input
        let m = v.match(/^(\d{3})(\d{3})(\d{4})$/);

        if (m) ev.target.value = '(' + m[1] + ") " + m[2] + "-" + m[3];
    }

    var phoneerror = validate(form, { Work_phone: { workPhoneValidator: ev.target.value } });
    return phoneerror ? showErrorsForInput(this, phoneerror[this.name]) : null;

});

streetInput.addEventListener("input", function (ev) {
    //Pass formatted user input through streetInputValidator
    var streeterror = validate(form, { Street_address: { streetInputValidator: ev.target.value } });
    return streeterror ? showErrorsForInput(this, streeterror[this.name]) : null;
});

zipCodeInput.addEventListener("input", function (ev) {
    //Pass formatted user input through zipCodeValidator
    var zipcodeerror = validate(form, { Zip_code: { zipCodeValidator: ev.target.value } });
    return zipcodeerror ? showErrorsForInput(this, zipcodeerror[this.name]) : null;
});

zipCodeFillInput.addEventListener("input", function (ev) {
    //Pass formatted user input through zipCodeValidator
    var zipCodeFillError = validate(form, { Zip_code_: { zipCodeValidator: ev.target.value } });
    return zipCodeFillError ? showErrorsForInput(this, zipCodeFillError[this.name]) : null;
});

dlnumberInput.addEventListener("input", function (ev) {
    var zipcodeerror = validate(form, { License_number: { licenseNumberValidator: ev.target.value } });
    return zipcodeerror ? showErrorsForInput(this, zipcodeerror[this.name]) : null;
});

var netMonthlyInput = document.getElementById("netamount");
netMonthlyInput.addEventListener("input", function (ev) {
    var netAmountError = validate(form, { Net_monthly_amount: { netAmountValidator: ev.target.value } });
    return netAmountError ? showErrorsForInput(this, netAmountError[this.name]) : null;
});

ssnInput.addEventListener("input", function (ev) {
    var value = ev.target.value.trim();
    var val = value.replace(/\D/g, '');
    var newVal = '';

    if (val.length > 4) {
        value = val;
    }

    if ((val.length > 3) && (val.length < 6)) {
        newVal += val.substr(0, 3) + '-';
        val = val.substr(3);
    }

    if (val.length > 5) {
        newVal += val.substr(0, 3) + '-';
        newVal += val.substr(3, 2) + '-';
        val = val.substr(5);
    }

    newVal += val;
    event.target.value = newVal.substring(0, 11).trim();

    var ssnerror = validate(form, { Social_security_number: { ssnValidator: ev.target.value } });
    return ssnerror ? showErrorsForInput(this, ssnerror[this.name]) : null;
});

dobInput.addEventListener("input", function (ev) {

    var doberror = validate(form, { Age: { dobValidator: ev.target.value } });
    return doberror ? showErrorsForInput(this, doberror[this.name]) : null;
});

routingNumberInput.addEventListener("input", function (ev) {
    var routingnumberError = validate(form, { Bank_routing_number: { routingNumberValidator: ev.target.value } });
    return routingnumberError ? showErrorsForInput(this, routingnumberError[this.name]) : null;

});
//End trigger custom validations on user input

//Formats DOB
dobInput.addEventListener("keyup", function (ev) {
    backSpace = (ev.key == "Backspace");
    var dobValue = ev.target.value;
    var v = dobValue;

    if (!backSpace) {
        if (v.match(/^\d{2}$/) != null) {
            dobValue = v + '/';
        } else if (v.match(/^\d{2}\/\d{2}$/) != null) {
            dobValue = v + '/';
        }
        ev.target.value = dobValue.replace(/\/\//g, '/'); //Replaces double slash '//' with single slash '/'
    }
    else {
        dobValue = ev.target.value;
        //validate(form, { Age: { dobValidator: ev.target.value } });
    }

});

//Formats DOB
dobInput.addEventListener("keyup", function (ev) {
    dobToEpoch(ev);
    //console.log(dobToEpoch(ev));
});

var emailInput = document.getElementById("email");
var errorBlock = document.querySelector("#email-div p.help-block.error");
var emailContainer = document.getElementById("email-div");

function showEmailDomains() {
    var userInput = emailInput.value;

    if (userInput.length > 2) {
        document.getElementById("email-button-block").classList.remove("hide_element");
    }
    else {
        document.getElementById("email-button-block").classList.add("hide_element");
    }

    if (emailContainer.classList.contains("has-success")) {
        document.getElementById("email-button-block").classList.add("hide_element");
    }
}

function addGmail() {
    var emailDomain = emailInput.value.indexOf('@');
    if (emailDomain !== -1) {
        emailInput.value = emailInput.value.slice(0, emailDomain);
    }

    if (document.querySelector("#email-div p.help-block.error")) {
        emailInput.value += "@gmail.com";
        document.getElementById("email-div").classList.remove("has-error");
        document.getElementById("email-div").classList.add("has-success");
        document.querySelector("#email-div p.help-block.error").innerHTML = '';
    }

    document.getElementById("email-button-block").classList.add("hide_element");
}

