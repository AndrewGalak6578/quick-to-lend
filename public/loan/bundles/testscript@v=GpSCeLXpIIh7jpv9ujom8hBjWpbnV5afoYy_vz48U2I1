            "ConsentToMarketingSms": document.getElementById("terms_confirm").checked,
            "ConsentToMarketingPhone": document.getElementById("terms_confirm").checked,
            "ResidentialStatus": Number(document.querySelector('input[name="radio_residential"]:checked').value),
            //"HouseNumber": document.getElementById('housenamenumber').value,
            "HouseName": null,
