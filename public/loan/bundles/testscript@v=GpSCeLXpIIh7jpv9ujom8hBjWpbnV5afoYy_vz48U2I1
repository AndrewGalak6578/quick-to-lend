    document.getElementById("bank-months").classList.remove("hide_element");
}
function hideBankMonths(event) {
    var monthsList = document.querySelectorAll('input[name="radio_bankMonths"]');
    monthsList.forEach(function (el) {
        el.checked = false;
    });
    document.getElementById("bank-months").classList.add("hide_element");
}

function togglePropertyValueField(event) {
    var inputVal = event.target.value;
    var propertyValueField = document.getElementById("property_value_field");
    var mortgageQuestion = document.getElementById("mortgage_question_field");

    if (inputVal == 1) {
        propertyValueField.classList.remove("hide_element");
        mortgageQuestion.classList.remove("hide_element");
    }
    else {
        propertyValueField.classList.add("hide_element");
        mortgageQuestion.classList.add("hide_element");

        document.getElementById("property_value").value = '';
        document.querySelector('input[name="radio_MortgageQuestion"]').checked = false;
    }
}

function toggleCurrentMortgageBalanceField(event) {
    var inputVal = event.target.value;
    var currentMortgageValueField = document.getElementById("current_mortgage_balance");

    if (inputVal == "true") {
        currentMortgageValueField.classList.remove("hide_element");
    }
    else {
        currentMortgageValueField.classList.add("hide_element");
        document.getElementById("mortgage_balance").value = '';
    }
}

function dobToEpoch() {
    var dateString = document.getElementById("dob").value; // mm/dd/yyyy format

    //if (dateString.length == 0) console.error("No DOB");

    return "\/Date(" + String(moment.utc(dateString, "MM/DD/YYYY").set({ hour: 12, minute: 0, second: 0, millisecond: 0 }).valueOf()) + ")\/";
}

function getIpAddress(url) {
    try {
        var xmlHttp = new XMLHttpRequest();
        xmlHttp.open("GET", url, false); // false for synchronous request
        xmlHttp.send(null);
        return xmlHttp.response.replace(/['"]+/g, '');
    } catch (e) {
        console.error('fetch of address');
    }

    return null;
}

function trackProgress(percentageComplete) {
    window.scrollTo(0, 0);//Scroll to top of page;

    document.getElementById("progressBar").classList.remove("hide_element");
    if (!document.getElementById("barStatus")) return;


    if (!percentageComplete || percentageComplete < 8) percentageComplete = "8";
    document.getElementById("barStatus").style.width = percentageComplete + "%";
    document.getElementById("barStatus").innerHTML = percentageComplete + "%";
    return false;
}

