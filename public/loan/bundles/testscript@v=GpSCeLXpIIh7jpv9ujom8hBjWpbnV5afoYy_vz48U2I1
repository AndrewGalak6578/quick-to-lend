        //trackingCode.setAttribute("src", "https://leads.pingyo.com/application/trackingcodes/" + correlationId);
        //console.log(document.getElementById("tracking-code"));
    } catch (error) {
        console.error(error);
        //TODO: Phone home and report error
    }
}

function startCountDown(correlationId, redirectionUrl, response) {
    //Inject tracking codes
    //var trackingCode = document.querySelector("iframe#tracking-code");
    //trackingCode.setAttribute("src", "https://leads.pingyo.com/application/trackingcodes/" + correlationId);
    //console.log(document.getElementById("tracking-code"));

    testPixels(response);
    trackFacebookConversion(response);

    if (navigator.userAgent.match("FB_IAB/FB4A")) {
        injectRedirectAsLink(redirectionUrl);
    }
    else {
        var timeleft = 5;
        var downloadTimer = setInterval(function () {
            if (timeleft < 1) {
                console.log(timeleft);
                //window.location.href = redirectionUrl;
                //window.location.href = "/offerwall/?id=" + pingyo.defaultAffiliate;
                //window.location.href = "/offerwall/?c=" + correlationId;
                //window.location.href = `/offerwall/?c=${correlationId}&a=${pingyo.defaultAffiliate}`;
                clearInterval(downloadTimer);
                return false;
            } else {
                document.getElementById("countdown").innerHTML = timeleft;
            }
            timeleft -= 1;
        }, 1000);
    }
}

function injectRedirectAsLink(redirectUrl) {
    var link = document.createElement("a");
    link.href = redirectUrl;
    link.title = "Redirect Link";
    link.className = "btn";
    link.innerHTML = "Proceed to your offer";

    document.getElementById("redirect-message").remove();
    document.getElementById("countdown").appendChild(link);
}

function getLoanProceedUse() {
    var loanPurpose = document.querySelector('input[name="radio_purpose"]:checked');
    var loanPurposeFromSelect = document.getElementById('loanPurposeFromSelect');

    if (loanPurpose.value != '0') return Number(loanPurpose.value);

    return Number(loanPurposeFromSelect.value);

}

function appendQueryParams(url, param, paramValue) {
    if ((!param && !paramValue) || url.indexOf(param) > 0)
        return url;

    var queryParams = url.indexOf('?') > 0;

    if (queryParams) {
        url = url + `&${param}=${paramValue}`;
    }
    else {
        url = url + `?${param}=${paramValue}`;
    }

    return url;
}

function isOther(event) {
    var val = event.target.value;

    if (val == "15") {
        document.getElementById("somethingelse-field").classList.remove("hide_element");
    }
    else {
        document.getElementById("somethingelse-field").classList.add("hide_element");
    }
}

function checkLoanProceed() {
    let loanProceedRadio = document.querySelector('input[name="loan-purpose-radio"]:checked');
    let loanProceedResult;

    if (loanProceedRadio.value == "15") {
        loanProceedResult = document.getElementById("loan-proceed-use-select").value;
    }
    else {
        loanProceedResult = loanProceedRadio.value;
    }
    return loanProceedResult;
}

function pullWorkNumber() {
    let workPhoneDiv = document.getElementById("workphone-field");
    let workPhoneInput = document.getElementById("workphone").value.replace(/\D/g, '');
    let mobilePhoneInput = document.getElementById("mobilephone");

    if (workPhoneDiv.classList.contains('hide_element')) {
        workPhoneInput = mobilePhoneInput.value.replace(/\D/g, '');
    }
    else {
        return workPhoneInput;
    }

    return workPhoneInput;
}




function processFormData() {
    var incomeValues = calculateIncome();
    var bankAccountOpened = calculateBank();
    var addressMoveIn = calculateMoveIn();
    var employername = document.getElementById('employername').value.length > 0 ? document.getElementById('employername').value : "Unemployed";
    var jobTitle = document.getElementById('jobtitle').value.length > 0 ? document.getElementById('jobtitle').value : "Unemployed";
    var dobEpoch = dobToEpoch();
    var referringUrl = "https://www.quick2lend.com";

    var loanProceed = checkLoanProceed();
    var workPhoneNumber = pullWorkNumber();

    if (!referringUrl) {
        referringUrl = 'https://www.quick2lend.com';
    }

    if (pingyo.cookies && pingyo.cookies.gclid)
        referringUrl = appendQueryParams(referringUrl, "gclid", pingyo.cookies.gclid);
    if (pingyo.cookies && pingyo.cookies.msclkid)
        referringUrl = appendQueryParams(referringUrl, "msclkid", pingyo.cookies.msclkid);

    var ipAddress = getIpAddress('https://www.pingyo.com/geo/echo/ip');

    let totalDebtString = document.getElementById('total-card-debt-select').value;
    let totalDebt = 0;

    if (!totalDebtString.length) {
        totalDebt = null;
    }
    else {
        totalDebt = Number(totalDebtString);
    }

    var applicationData = {
        "Campaign": pingyo.defaultCampaign,
        "AffiliateId": pingyo.defaultAffiliate,
        "SubAffiliate": pingyo.defaultSubAffiliate,
        "Timeout": 0,
        "TestOnly": false,
        "Application": {
            "Title": Number("1"),
            "FirstName": document.getElementById('firstname').value,
            "LastName": document.getElementById('lastname').value,
            "DateOfBirth": dobEpoch,
            "Email": document.getElementById('email').value,
            "HomePhoneNumber": document.getElementById('mobilephone').value.replace(/\D/g, ''), //Set same as mobile phone if user skips optional home phone field
            "MobilePhoneNumber": document.getElementById('mobilephone').value.replace(/\D/g, ''),
            "WorkPhoneNumber": workPhoneNumber,
            "EmployerName": employername,
            "JobTitle": jobTitle,
            "EmploymentStarted": incomeValues.employmentstarted,
            "IncomeSource": Number(document.querySelector('input[name="radio_source"]:checked').value),
            "PayFrequency": Number(document.querySelector('input[name="radio_frequency"]:checked').value),
            "PayAmount": Number(document.getElementById('netamount').value.replace(/,/g, "")),
            "IncomePaymentType": Number(document.querySelector('input[name="radio_payment"]:checked').value),
            "NextPayDate": incomeValues.nextpaydate,
            "FollowingPayDate": incomeValues.followingpaydate,
            "LoanAmount": Number(document.getElementById('la').value.replace(/\D/g, '')),
            "Term": Number("3"),
            "MilitaryService": document.getElementById("military").checked ? 4 : 1,
            "NationalIdentityNumber": document.getElementById("ssn").value.replace(/\D/g, ''),
            "NationalIdentityNumberType": 2,
            "DriversLicenseNumber": document.getElementById("dlnumber").value,
            "DriversLicenseState": document.getElementById("dl-state-select").value,
            "LoanProceedUse": Number(loanProceed),
            "ConsentToCreditSearch": true,
            "ConfirmedByApplicant": true,
            "ConsentToMarketingEmails": document.getElementById("terms_confirm").checked,
            "ConsentToMarketingSms": document.getElementById("terms_confirm").checked,
            "ConsentToMarketingPhone": document.getElementById("terms_confirm").checked,
            "ResidentialStatus": Number(document.querySelector('input[name="radio_residential"]:checked').value),
            //"HouseNumber": document.getElementById('housenamenumber').value,
            "HouseName": null,
            "AddressStreet1": document.getElementById('housestreet').value,
            "AddressCity": document.getElementById('city').value,
            "AddressCountryCode": "US",
            "AddressCounty": document.getElementById('county').value,
            "AddressState": document.getElementById('state-select').value,
            "AddressMoveIn": addressMoveIn,
            "AddressPostcode": document.getElementById('zipcode').value.slice(0, 5),
            "BankName": document.getElementById("bankname").value.trim(),
            "BankAccountNumber": document.getElementById('accountnumber').value,
            "BankAccountOpened": bankAccountOpened,
            "BankCardType": 5, //Hardcoding to 'debit' for US,
            "BankAccountType": 1,
            "BankRoutingNumber": document.getElementById('routingnumber').value,
            "MinimumCommissionAmount": 0,
            "MaximumCommissionAmount": 0,
            "ApplicationExtensions": pingyo.appExtensions,
            "TotalCreditCommitments": totalDebt
        },
        "SourceDetails": {
            "Address": ipAddress,
            "ClientUserAgent": navigator.userAgent,
            "CreationUrl": referringUrl,
            "LanguageCodes": ["en-US"],
            "ReferringUrl": referringUrl
        }
    };
    console.log(JSON.stringify(applicationData));
    return JSON.stringify(applicationData);
}

function processFormDataSpecialOffer() {
    var incomeValues = calculateIncome();
    var bankAccountOpened = calculateBank();
    var addressMoveIn = calculateMoveIn();
    var employername = document.getElementById('employername').value.length > 0 ? document.getElementById('employername').value : "Unemployed";
    var jobTitle = document.getElementById('jobtitle').value.length > 0 ? document.getElementById('jobtitle').value : "Unemployed";
    var dobEpoch = dobToEpoch();
    var referringUrl = "https://www.quick2lend.com";

    var loanProceed = checkLoanProceed();
    var workPhoneNumber = pullWorkNumber();

    if (!referringUrl) {
        referringUrl = 'https://www.quick2lend.com';
    }

    if (pingyo.cookies && pingyo.cookies.gclid)
        referringUrl = appendQueryParams(referringUrl, "gclid", pingyo.cookies.gclid);
    if (pingyo.cookies && pingyo.cookies.msclkid)
        referringUrl = appendQueryParams(referringUrl, "msclkid", pingyo.cookies.msclkid);

    let ipAddress = getIpAddress('https://www.pingyo.com/geo/echo/ip');

    let totalDebtString = document.getElementById('total-card-debt-select').value;
    let totalDebt = 0;

    if (!totalDebtString.length) {
        totalDebt = null;
    }
    else {
        totalDebt = Number(totalDebtString);
    }

    let offerTypeId = Number(document.getElementById("offerTypeId").value);

    var applicationData = {
        "Campaign": pingyo.defaultCampaign,
        "AffiliateId": pingyo.defaultAffiliate,
        "SubAffiliate": pingyo.defaultSubAffiliate,
        "Timeout": 0,
        "TestOnly": false,
        "Application": {
