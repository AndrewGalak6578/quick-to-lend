}

function handleSuccessWithOffer() {
    //Gathers form data
    var formData = processFormDataSpecialOffer();
    //console.log(formData);
    //return false; //For development

    closeSpecialOffer(false);

    //Show progress section
    progressDiv.classList.add("open");
    progressDiv.style.display = null;
    document.getElementsByTagName("body")[0].scrollIntoView({ behavior: "auto" });
    //Show progress section

    trackFacebookSubmission();

    //Submits form
    var submitUrl = "https://pt-publicproxy-us.pingyo.com/lapplication/us/submit";
    fetch(submitUrl, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: formData,
    })
        .then((response) => response.json())
        .then((data) => {
            var statusCheckPath = data.StatusCheckUrl;

            if (statusCheckPath === undefined || statusCheckPath === null || statusCheckPath == '') {
                handleErredResponse(data);
            }
            else {
                var statusUrl = "https://pt-publicproxy-us.pingyo.com" + statusCheckPath;

                var progressInterval = setInterval(() => {
                    var response;

                    fetch(statusUrl, {
                        method: 'GET',
                        headers: {
                            'Content-Type': 'application/json',
                        }
                    })
                        .then((response) => response.json())
                        .then((data) => {
                            response = data;
                            //console.log(response);
                            var correlationId = response.CorrelationId;
                            var percentageComplete = response.PercentageComplete;
                            var status = response.Status;
                            var redirectionUrl = response.RedirectionUrl;

                            //Show progress bar
                            trackProgress(percentageComplete);

                            if (redirectionUrl) {
                                console.debug("redirecting")
                                progressDiv.classList.remove("open");
                                countdownDiv.classList.remove("hide_element");
                                startCountDown(correlationId, redirectionUrl, response);
                                clearInterval(progressInterval);
                                return false;
                            }
                            else if (status == "NoLenderMatchFound" || (percentageComplete == 100 && !redirectionUrl)) {
                                countdownDiv.classList.add("hide_element");
                                console.debug("no lender");

                                progressDiv.classList.add("open");
                                statusText.innerHTML = "Could not match you with a lender at this time. Please try again.";

                                //window.location.href = "https://www.pingyo.com/usa_offers";
                                clearInterval(progressInterval);
                                return;
                            }
                        })
                        .catch((error) => {
                            clearInterval(progressInterval);

                            console.error('Error:', error);

                            handleErredResponse(data);
                        });
                }, 2500);
            }
        })
        .catch((error) => {
            console.error('Error:', error);
        });
}

function submitFormWithOfferCheck() {
    if (offerCheckbox.checked) {
        submitFormWithOffer();
    }
    else {
        submitForm();
    }
}

function submitForm() {
    closeSpecialOffer(false);

    nextPrev(1);
    var valid = validateForm();
    //console.log("test " + valid);
    if (valid) {
        handleSuccess();
    }
    //return false;
}

function submitFormWithOffer() {
    closeSpecialOffer(false);

    nextPrev(1);
    var valid = validateForm();
    //console.log("test " + valid);
    if (valid) {
        handleSuccessWithOffer();
    }
    //return false;
}

function trackFacebookSubmission() {
    try {
        fbq('track', 'SubmitApplication');
    } catch (error) {
        console.error(error);
    }
}

function trackFacebookConversion(response) {
    try {
        fbq('track', 'Purchase', { value: response.EstimatedCommission.Amount, currency: 'USD' });
    } catch (error) {
        console.error(error);
    }
}

function testPixels(response) {
    console.warn({ response, pingyo });

    try {
        if (pingyo.defaultAffiliate == "BNG-QUICK2L") {
            bingPixel(response.EstimatedCommission.Amount);
        }
    } catch (error) {
        console.error(error);
        //TODO: Phone home and report error
    }
    try {
        if (pingyo.defaultAffiliate == "GGL-QUICK2L") {
            googlePixel(response.EstimatedCommission.Amount, response.CorrelationId);
        }
    } catch (error) {
        console.error(error);
        //TODO: Phone home and report error
    }
}

function bingPixel(Application_Affiliate_Commission) {
    window.uetq = window.uetq || [];
    window.uetq.push("event", "purchase", { revenue_value: Application_Affiliate_Commission, currency: "USD" });
}

function googlePixel(Application_Affiliate_Commission, Application_ReferenceToken) {

    //console.warn("GOOGLE PIXEL FIRING");

    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    //gtag('config', 'AW-821098910');

    gtag('event', 'conversion', {
        'send_to': 'AW-821098910/RFJ3CP7-sHsQnvPDhwM',
        'value': Application_Affiliate_Commission,
        'currency': 'USD',
        'transaction_id': Application_ReferenceToken
    });

}
//For handling next/prev and general validation
var currentSection = 0; // Current tab is set to be the first tab (0)
showSection(currentSection); // Display the current tab

function showSection(n) {
    //// This function will display the specified tab of the form ...
    var form = document.getElementById("pingYoForm");
    var x = document.getElementsByClassName("section-form");

    if (x[n] == undefined) return;

    x[n].style.display = "block";

    form.scrollIntoView();
    window.scrollBy({ top: -70, behavior: 'smooth' }); //Offsets height

    fixStepIndicator(n);
}

function nextPrev(n) {
    if(n == 2){
        var RequestSpecialOffer = document.getElementById("RequestSpecialOffer");
        if(!RequestSpecialOffer.checked){
            showCreditModal();
            return;
        }
    }
    // This function will figure out which tab to display
    var x = document.getElementsByClassName("section");

    // Exit the function if any field in the current tab is invalid:
    if (n !== -1 && !validateForm()) return false;

    // Hide the current tab:
    if (x[currentSection])
        x[currentSection].style.display = "none";

    // Increase or decrease the current tab by 1:
    currentSection = currentSection + n;

    // Otherwise, display the correct tab:
    showSection(currentSection);
}

function validateForm() {
    // This function deals with validation of the form fields
    var x, y, i, valid = true;
    var radioValid = false;

    x = document.getElementsByClassName("section-form");
    //console.log(x[currentSection]);
    if (x[currentSection] === undefined) {
        return true;
    }
    else {
        y = x[currentSection].querySelectorAll("input, textarea, select");
    }

    var radioCollection = x[currentSection].querySelectorAll("div.radio-class");
    var filterOutFields = x[currentSection].querySelectorAll(".hide_element input, .hide_element select, #userAddressSelect, #homephone, .no-validate input");

    var filterOutJobRadio = x[currentSection].querySelectorAll(".hide_element div.radio-class.radio_employer, .hide_element .radio-class");
    var hasError = x[currentSection].querySelectorAll(".has-error");

    filterOutFields = [].slice.call(filterOutFields); //Convert to array
    filterOutJobRadio = [].slice.call(filterOutJobRadio); //Convert to array

    //Remove job fields input, select elements if they have 'hide_element' as class name
    //[].slice.call(nodeList) converts node list into an array => for mapping or filtering
    y = [].slice.call(y).filter(
        function (e) {
            return this.indexOf(e) < 0;
        },
        filterOutFields
    );

    //Remove radio inputs
    radioCollection = [].slice.call(radioCollection).filter(
        function (e) {
            return this.indexOf(e) < 0;
        },
        filterOutJobRadio
    );

    //Make sure there are no validation errors
    if (hasError.length > 0) {
        valid = false;
    }
    else {
        valid = true;
    }

    // A loop that checks every input, select field in the current tab:
    for (i = 0; i < y.length; i++) {
        // If a field is empty...
        if (y[i].value == "") {
            // add an "invalid" class to the field:
            y[i].className += " invalid";
            // and set the current valid status to false:
            valid = false;
        }
        else {
            y[i].classList.remove("invalid");
        }
    }

    var selectedRadioCount = 0; //Start counter
    //Loop through collection of radio buttons and sees if user has selected one
    if (radioCollection.length) {
        radioCollection.forEach(function (element) {
            var radioArr = element.querySelectorAll("input");
            var checkedRadios = [];

            radioArr.forEach(function (nestedElement) {
                checkedRadios.push(nestedElement.checked);
            });
            if (checkedRadios.indexOf(true) < 0) {
                element.parentNode.classList.add("invalid");
            }
            else {
                element.parentNode.classList.remove("invalid");
                selectedRadioCount++; //Add to count if user selected radio
            }
        });
    }
    else {
        //Mark radioValid as true if no radio button groups exist in current tab
        radioValid = true;
    }
    //Loop through collection of radio buttons and sees if user has selected one

    //Mark radios as valid if all groups have a selection
    if (selectedRadioCount == radioCollection.length) {
        radioValid = true;
    }

    if (valid && radioValid) {
        return true;
    }
    else {
        //Scrolls to first invalid or has-error class
        var firstError = document.querySelectorAll(".invalid, .has-error")[0];
        firstError.scrollIntoView();
        window.scrollBy({ top: -200, behavior: 'smooth' }); //Offsets height by 250px

        //console.log(document.querySelectorAll(".invalid, .has-error"));
    }

    return valid && radioValid; // return the valid status
    //return true; // return the valid status => For development
}

function fixStepIndicator(n) {
    // This function removes the "active" class of all steps...
    var i, x = document.getElementsByClassName("section");

    for (i = 0; i < x.length; i++) {
        x[i].className = x[i].className.replace(" active", "");
    }
    //... and adds the "active" class to the current step:
    x[n].className += " active";
}
//For handling next/prev and general validation

function toCapitalize(event) {
    if (!event) return;
    var val = event.target.value;
    event.target.value = val.charAt(0).toUpperCase() + val.slice(1);
}

function toUpperCase(event) {
    if (!event) return;
    var val = event.target.value;

    event.target.value = val.toUpperCase();
}

function getZipCodeInfo(event) {
    if (!event) return;
    if (event.key == "Enter") return;
    if (event.key == "Backspace") {
        //console.error("Backspace, removing area code buttons");
        var areaCodeButtons = document.querySelectorAll("button.areacode-button");
        areaCodeButtons.forEach((el) => {
            el.remove()
        })
    };

    var val = event.target.value.trim();
    var url = "https://www.pingyo.com/find/locales/zipcode/" + val;

    if (val.length === 5) {
        var xmlHttp = new XMLHttpRequest();
        xmlHttp.open("GET", url, true); // false for synchronous request

        xmlHttp.onload = function (e) {
            if (xmlHttp.readyState === 4) {
                if (xmlHttp.status === 200) {
                    //Success action
                    var returnArray = JSON.parse(xmlHttp.responseText);

                    //Removes Duplicates
                    var result = returnArray.reduce((unique, o) => {
                        if (!unique.some((obj) => obj.AreaCode === o.AreaCode)) {
                            unique.push(o);
                        }
                        return unique;
                    }, []);

                    fillZipCodeInfo(result)

                } else {
                    console.warn(xmlHttp.statusText);
                }
            }
        };
        xmlHttp.onerror = function (e) {
            console.error(xmlHttp.statusText);
        };
        xmlHttp.send(null);
    }

}

var cellPhoneFill = document.getElementById("cellphone-fill");
var workPhoneFill = document.getElementById("workphone-fill");

var cellPhone = document.getElementById("mobilephone");
var workPhone = document.getElementById("workphone");

function checkMobileNumber(event) {
    var mobile = document.getElementById("mobilephone").value;
    //var home = document.getElementById("homephone").value;
    var work = document.getElementById("workphone").value;
    var errorDiv = document.getElementById("same-number-warning");

    var thisVal = event.target.value;

    if (thisVal == work) {
        //console.warn("same number (mobile)");
        errorDiv.classList.remove("hide_element");
    }
    else {
        errorDiv.classList.add("hide_element");
    }

    if (thisVal.length > 1) {
        cellPhoneFill.classList.add("hide_element");
    }
}

function checkWorkNumber(event) {
    var mobile = document.getElementById("mobilephone").value;
    //var home = document.getElementById("homephone").value;
    var work = document.getElementById("workphone").value;
    var errorDiv = document.getElementById("same-number-warning");

    var thisVal = event.target.value;

    if (thisVal == mobile) {
        //console.warn("same number");
        errorDiv.classList.remove("hide_element");
    }
    else {
        errorDiv.classList.add("hide_element");
    }

    if (thisVal.length > 1) {
        workPhoneFill.classList.add("hide_element");
    }
}

function fillAreaCode(infoArray) {
    infoArray.forEach((el) => {
        var cellButton = document.createElement("button");
        var areaCode = el.AreaCode;

        cellButton.type = "button";
        cellButton.classList.add("btn")
        cellButton.classList.add("btn-default");
        cellButton.classList.add("areacode-button");
        cellButton.textContent = "(" + areaCode + ")";

        cellPhoneFill.insertAdjacentElement("beforeend", cellButton);

        var workButton = document.createElement("button");
        var areaCode = el.AreaCode;

        workButton.type = "button";
        workButton.classList.add("btn")
        workButton.classList.add("btn-default");
        workButton.classList.add("areacode-button");
        workButton.textContent = "(" + areaCode + ")";;

        workPhoneFill.insertAdjacentElement("beforeend", workButton);

        cellButton.addEventListener("click", function () {
            cellPhone.value = "(" + areaCode + ")";
            cellPhone.focus();
            cellPhoneFill.classList.add("hide_element");
            //console.error(areaCode);
        });

        workButton.addEventListener("click", function () {
            workPhone.value = "(" + areaCode + ")";
            workPhone.focus();
            workPhoneFill.classList.add("hide_element");
            //console.error(areaCode);
        });

        var event = new Event('input', {
            bubbles: true,
            cancelable: true,
        });

        //Triggers validation
        cellPhone.dispatchEvent(event);
        workPhone.dispatchEvent(event);
    })
}

function fillZipCodeInfo(infoArray) {
    if (!infoArray) return;
    console.debug(infoArray);

    if (infoArray.length == 1) {
        var areaCode = infoArray[0].AreaCode;

        document.getElementById("mobilephone").value = "(" + areaCode + ")";
        document.getElementById("workphone").value = "(" + areaCode + ")";

        var event = new Event('input', {
            bubbles: true,
            cancelable: true,
        });

        //Triggers validation
        cellPhone.dispatchEvent(event);
        workPhone.dispatchEvent(event);
    }
    else {
        document.getElementById("mobilephone").value = ""
        document.getElementById("workphone").value = "";

        cellPhoneFill.classList.remove("hide_element");
        workPhoneFill.classList.remove("hide_element");

        fillAreaCode(infoArray);

    }

    document.getElementById("city").value = infoArray[0].City;
    document.getElementById("county").value = infoArray[0].County;
    document.getElementById("zipcode").value = infoArray[0].PostalCode;
    document.getElementById("state-select").value = infoArray[0].StateAbbreviation;
    document.getElementById("dl-state-select").value = infoArray[0].StateAbbreviation;

    //For CCPA notice
    document.getElementById("state-select").dispatchEvent(new Event("change"));
    document.getElementById("dl-state-select").dispatchEvent(new Event("change"));
}

//Need to add job fields dynamically to work with validation
var jobFields = document.getElementById("job_fields");
var incomeMessageDiv = document.getElementById("income-message");
function showJobFields(event) {
    var messageHidden = incomeMessageDiv.classList.contains("hide_element");

    var workphonefield = document.getElementById("workphone-field");
    workphonefield.classList.remove("hide_element");
    workphonefield.classList.add("show_element");


    var val = event.target.value;
    toggleNetGrossMonthly(event);
    jobFields.classList.remove("hide_element");
    jobFields.classList.add("show_element");

    document.getElementById("netamount").value = "";
    //hideRetirementPensionField();
    //hideGovernmentBenefitsField();
    //document.getElementById("other_income_checkbox").checked = false;
    //document.getElementById("other_benefits_field").classList.add("hide_element");

    if (!messageHidden) {
        hideIncomeMessage(val);
    }
}

//Need to remove job fields dynamically to work with validation
function hideJobFields(event) {
    var messageHidden = incomeMessageDiv.classList.contains("hide_element");

    var workphonefield = document.getElementById("workphone-field");
    workphonefield.classList.remove("show_element");
    workphonefield.classList.add("hide_element");

    var zipCodeField = document.getElementById("zipcode-field");
    if (!zipCodeField.classList.contains('hide_element')) {
        document.getElementById("workphone").value = "";
        workphonefield.classList.remove("has-error");
    }

    let val = event.target.value;
    jobFields.classList.remove("show_element");
    jobFields.classList.add("hide_element");
    //console.log(messageHidden);

    if (val > 4) {
        document.getElementById("employername").value = "";
        document.getElementById("jobtitle").value = "";
    }

    //Retirement/Pension
    if (val == 5) {
        document.getElementById("netamount").value = "0";
    }
    //Government Benefits
    if (val == 6 || val == 7) {
        document.getElementById("netamount").value = "0";
    }

    if (messageHidden) {
        showIncomeMessage(val);
    }

    var event = new Event('input', {
        bubbles: true,
        cancelable: true,
    });

    //Triggers validation
    document.getElementById("netamount").dispatchEvent(event);
}
function showIncomeMessage(val) {
    incomeMessageDiv.classList.remove("hide_element");

    if (val != 1) {
        //    var xhr = new XMLHttpRequest();
        //    xhr.open("POST", "https://leads.pingyo.com/application/metric/benefitsincomemessage/display", true);
        //    xhr.send('');
    }
}

function hideIncomeMessage(val) {
    incomeMessageDiv.classList.add("hide_element");

    if (val != 1) {
        //    var xhr = new XMLHttpRequest();
        //    xhr.open("POST", "https://leads.pingyo.com/application/metric/benefitsincomemessage/change", true);
        //    xhr.send('');
    }
}

function showEmployerMonths(event) {
    document.getElementById("employer-months").classList.remove("hide_element");
}
function hideEmployerMonths(event) {
    var monthsList = document.querySelectorAll('input[name="radio_employerMonths"]');
    monthsList.forEach(function (el) {
        el.checked = false;
    });
    document.getElementById("employer-months").classList.add("hide_element");
}

function showAddressMonths(event) {
    document.getElementById("address-months").classList.remove("hide_element");
}
function hideAddressMonths(event) {
    var val = Number(event.target.value);
    var monthsList = document.querySelectorAll('input[name="radio_addressMonths"]');
    monthsList.forEach(function (el) {
        el.checked = false;
    });
    document.getElementById("address-months").classList.add("hide_element");
}

