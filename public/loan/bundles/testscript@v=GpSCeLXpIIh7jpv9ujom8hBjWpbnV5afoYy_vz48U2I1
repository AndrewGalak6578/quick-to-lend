                                    return (
                                        (this.positionAbs = s.positionAbs),
                                        (this.helperProportions = s.helperProportions),
                                        (this.offset.click = s.offset.click),
                                        this !== o && this._intersectsWith(this.containerCache) && t.contains(o.element[0], this.element[0]) && (n = !1),
                                        n
