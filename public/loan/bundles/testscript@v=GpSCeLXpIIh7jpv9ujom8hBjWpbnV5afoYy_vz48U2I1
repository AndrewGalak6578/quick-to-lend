                    for (i = u.length - 1; i >= 0; i--) for (r = u[i][1], s = 0, h = (a = u[i][0]).length; s < h; s++) (l = t(a[s])).data(this.widgetName + "-item", r), c.push({ item: l, instance: r, width: 0, height: 0, left: 0, top: 0 });
                },
                refreshPositions: function (e) {
                    var i, s, n, o;
                    for (
                        this.floating = !!this.items.length && ("x" === this.options.axis || this._isFloating(this.items[0].item)),
                        this.offsetParent && this.helper && (this.offset.parent = this._getParentOffset()),
                        i = this.items.length - 1;
                        i >= 0;
                        i--
                    )
                        ((s = this.items[i]).instance !== this.currentContainer && this.currentContainer && s.item[0] !== this.currentItem[0]) ||
